{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@antv/l7": "^2.4.3",
    "@antv/l7-maps": "^2.4.3",
    "@antv/l7-react": "^2.3.0",
    "@umijs/preset-react": "1.x",
    "umi": "^3.4.25",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@umijs/test": "^3.4.25",
    "autoprefixer": "^9",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "lint-staged": "^10.0.7",
    "postcss": "^7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
